<Page x:Class="z1.View.pageVlasnik"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:z1.View"
      mc:Ignorable="d" 
      DataContext="{Binding RelativeSource={RelativeSource Self}}"
      d:DesignHeight="450" d:DesignWidth="800"
      Title="pageVlasnik">
  

    <StackPanel>
        <StackPanel>
            <TextBlock Text="Id"/>
            <TextBox Text="{Binding NoviVlasnik.Id}"/>
            <TextBlock Text="Ime"/>
            <TextBox Text="{Binding NoviVlasnik.Ime}"/>
            <TextBlock Text="Prezime"/>
            <TextBox Text="{Binding NoviVlasnik.Prezime}"/>
            <Button Content="Dodaj" Margin="5" Click="Button_Click"/>
        </StackPanel>
        <ComboBox ItemsSource="{Binding Vlasnici}"/>
    </StackPanel>
</Page>

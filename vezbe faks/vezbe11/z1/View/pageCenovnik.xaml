<Page x:Class="z1.View.pageCenovnik"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:z1.View"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="pageCenovnik">

    <StackPanel>
        <TextBlock Text="Cena parkinga po satu"/>
        <TextBox Text="{Binding Cenovnik.CenaSat}"/>
        <TextBlock Text="Cena parkinga za ceo dan"/>
        <TextBox Text="{Binding Cenovnik.CenaDan}"/>
        <TextBlock Text="Broj parking mesta"/>
        <TextBox Text="{Binding Cenovnik.BrMesta}"/>
        <Button Content="Snimi" Click="Button_Click"/>
    </StackPanel>
</Page>

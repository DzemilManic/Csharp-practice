<Window x:Class="Vojska.View.SacuvajDat"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Vojska.View"
        mc:Ignorable="d"
        Title="SacuvajDat" Height="450" Width="400">
    <StackPanel Orientation="Vertical">
        <TextBlock Text="Napisi putanju za cuvanje" Margin="10"/>
        <TextBox  x:Name="tbPath" Margin="10"/>
        <Button Height="30" Margin="10" Click="Button_Click" Content="Snimi"/>
    </StackPanel>
</Window>
